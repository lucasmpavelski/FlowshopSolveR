        {
            "name": "fsp_solver_example",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceFolder}/build/main/fsp_solver",
            "args": [
                "--data_folder=${workspaceFolder}/data",
                "--mh=IG",
                "--problem_names=problem,type,objective,budget,stopping_criterium,instance",
                "--problem_values=FSP,PERM,MAKESPAN,med,FIXEDTIME,taillard_rand_100_10_01.dat",
                "--params_names=IG.Init,IG.Init.NEH.Priority,IG.Init.NEH.PriorityOrder,IG.Init.NEH.PriorityWeighted,IG.Init.NEH.Insertion,IG.Comp.Strat,IG.Neighborhood.Size,IG.Neighborhood.Strat,IG.Local.Search,IG.Accept,IG.Accept.Temperature,IG.Algo,IG.Destruction.Size,IG.LS.Single.Step,IG.LSPS.Local.Search,IG.LSPS.Single.Step,IG.AOS.Strategy,IG.AOS.RewardType",
                "--params_values=neh,sum_pij,incr,0,first_best,0,9.9999,random,3,temperature,0.5,adaptive,4,0,0,0,frrmab,1",
                "--printBestFitness"
            ],
        },
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/main/refine" ,
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
