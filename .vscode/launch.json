    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceFolder}/build/test/test-aos",
            "args": [],
        },
        {
            "name": "fsp_solver_example",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceFolder}/build/main/fsp_solver",
            "args": [
                "--data_folder=${workspaceFolder}/data",
                "--printBestFitness",
                "--problem=FSP",
                "--type=PERM",
                "--objective=MAKESPAN",
                "--budget=med",
                "--stopping_criterium=FIXEDTIME",
                "--instance=taillard_rand_100_10_01.dat",
                "--mh=IG",
                "--IG.Init=neh",
                "--IG.Init.NEH.Priority=sum_pij",
                "--IG.Init.NEH.PriorityOrder=incr",
                "--IG.Init.NEH.PriorityWeighted=0",
                "--IG.Init.NEH.Insertion=first_best",
                "--IG.Comp.Strat=strict",
                "--IG.Neighborhood.Size=1",
                "--IG.Neighborhood.Strat=random",
                "--IG.Local.Search=best_insertion",
                "--IG.LS.Single.Step=0",
                "--IG.Accept=temperature",
                "--IG.Accept.Temperature=0.5",
                "--IG.Perturb=rs",
                "--IG.Perturb.DestructionSize=4",
                "--IG.Perturb.Insertion=first_best",
                "--IG.LSPS.Local.Search=best_insertion",
                "--IG.LSPS.Single.Step=0",
                "--IG.AOS.Strategy=frrmab",
                "--IG.AOS.RewardType=1",
            ],
        },
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/main/refine" ,
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
