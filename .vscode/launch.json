    "version": "0.2.0",
    "configurations": [                
        {
            "name": "fsp_solver_example",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceFolder}/build/main/fsp_solver",
            "args": [
                " --data_folder=/home/lucasmp/dev/ig-aos-flowshop/data ",
                " --seed=654 ",
                " --problem=FSP ",
                " --type=NOWAIT ",
                " --objective=MAKESPAN ",
                " --budget=med ",
                " --stopping_criterion=FIXEDTIME ",
                " --instance=taill-like_rand_30_20_03.dat ",
                " --mh=IG ",

                " --NEH.Init=neh ",
                " --NEH.Init.NEH.Priority=ss_sra ",
                " --NEH.Init.NEH.PriorityOrder=lo_hilo ",
                " --NEH.Init.NEH.PriorityWeighted=1 ",
                " --NEH.Init.NEH.Insertion=last_best ",

                " --IG.Init=neh ",
                " --IG.Init.NEH.Priority=sum_pij ",
                " --IG.Init.NEH.PriorityOrder=incr ",
                " --IG.Init.NEH.PriorityWeighted=0 ",
                " --IG.Init.NEH.Insertion=first_best ",
                " --IG.Comp.Strat=strict ",
                " --IG.Neighborhood.Size=1 ",
                " --IG.Neighborhood.Strat=ordered ",
                " --IG.Local.Search=none ",
                " --IG.LS.Single.Step=0 ",
                " --IG.Accept=temperature ",
                " --IG.Accept.Temperature=0.8 ",
                " --IG.Perturb=lsps ",
                " --IG.Perturb.DestructionSizeStrategy=adaptive ",
                " --IG.Perturb.DestructionSize=NA ",
                " --IG.Perturb.Insertion=first_best ",
                " --IG.LSPS.Local.Search=none ",
                " --IG.LSPS.Single.Step=0 ",
                " --IG.AOS.Strategy=probability_matching ",
                " --IG.AOS.RewardType=1 ",
                " --IG.AOS.Options=1_2 ",
                " --IG.AOS.PM.RewardType=avgnorm ",
                " --IG.AOS.PM.Alpha=0.3647 ",
                " --IG.AOS.PM.PMin=0.1697 ",
                " --IG.AOS.PM.UpdateWindow=453 ",
                " --IG.AOS.FRRMAB.WindowSize=NA ",
                " --IG.AOS.FRRMAB.Scale=NA ",
                " --IG.AOS.FRRMAB.Decay=NA ",
                " --IG.AOS.LINUCB.Alpha=NA ",
                " --IG.AOS.WarmUp.Proportion=0.2721 ",
                " --IG.AOS.WarmUp.Strategy=fixed ",
                " --IG.AOS.TS.Strategy=NA ",
                " --IG.AOS.TS.C=NA ",
                " --printBestFitness ",
            ],
        },                
        {
            "name": "test-fsp",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceFolder}/build/test/test-fsp",
            "args": [],
        },
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/main/refine",
            "args": [
                "localLocal"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
